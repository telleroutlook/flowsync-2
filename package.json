{
  "name": "flowsync-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev",
    "lint": "tsc --noEmit",
    "build": "npm run build:prod",
    "build:prod": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "deploy": "npm run build:prod && wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply flowsync --local",
    "db:migrate:prod": "wrangler d1 migrations apply flowsync --remote",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "drizzle-orm": "^0.44.2",
    "framer-motion": "^12.29.0",
    "hono": "^4.6.9",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.35.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lru-cache": "^11.2.5",
    "lucide-react": "^0.563.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250124.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.18",
    "wrangler": "^4.60.0"
  }
}
